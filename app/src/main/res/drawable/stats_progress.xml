<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate android:fromDegrees="270" android:toDegrees="270">
            <shape
                android:innerRadiusRatio="3"
                android:shape="ring"
                android:thickness="12dp"
                android:useLevel="true">
                <solid android:color="#D7FBC7C3" />
            </shape>
        </rotate>
    </item>
    <item android:bottom="265dp">
        <shape
            android:innerRadiusRatio="1200"
            android:shape="ring"
            android:thickness="6dp"
            android:useLevel="false">
            <solid android:color="#D7FBC7C3" />
        </shape>
    </item>
    <item>
        <rotate>
            <inset android:insetBottom="265dp">
                <shape
                    android:innerRadiusRatio="1200"
                    android:shape="ring"
                    android:thickness="6dp"
                    android:useLevel="false">
                    <solid android:color="#D7FBC7C3" />
                </shape>
            </inset>
        </rotate>
    </item>
</layer-list>